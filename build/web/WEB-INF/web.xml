<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Verify</servlet-name>
        <servlet-class>Verify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>yamini</servlet-name>
        <servlet-class>yamini</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendEmail</servlet-name>
        <servlet-class>.SendEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerify</servlet-name>
        <servlet-class>newpackage.UserVerify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyCode</servlet-name>
        <servlet-class>newpackage.VerifyCode</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerify1</servlet-name>
        <servlet-class>newpackage.UserVerify1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerify2</servlet-name>
        <servlet-class>newpackage.UserVerify2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Verify</servlet-name>
        <url-pattern>/Verify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>yamini</servlet-name>
        <url-pattern>/yamini</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendEmail</servlet-name>
        <url-pattern>/SendEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserVerify</servlet-name>
        <url-pattern>/UserVerify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyCode</servlet-name>
        <url-pattern>/VerifyCode</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserVerify1</servlet-name>
        <url-pattern>/UserVerify1</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>UserVerify2</servlet-name>
        <url-pattern>/UserVerify2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
